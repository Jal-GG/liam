{
  "name": "@liam-hq/erd-web",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "@liam-hq/db-structure": "workspace:*",
    "@liam-hq/erd-core": "workspace:*",
    "@liam-hq/ui": "workspace:*",
    "@next/third-parties": "15.1.5",
    "@sentry/nextjs": "8",
    "cheerio": "1.0.0",
    "date-fns": "4.1.0",
    "next": "15.1.2",
    "react": "18.3.1",
    "react-dom": "18",
    "valibot": "^1.0.0-beta.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/gtag.js": "0.0.20",
    "@types/node": "22.9.0",
    "@types/react": "18",
    "@types/react-dom": "18",
    "typed-css-modules": "0.9.1",
    "typescript": "5"
  },
  "scripts": {
  "build": "pnpm run gen && next build",
  "dev": "pnpm run dev:next",
  "dev:css": "tcm components --watch",
  "dev:next": "next dev --port 3001",
  "fmt": "pnpm run fmt:biome",
  "fmt:biome": "biome check --write --unsafe .",
  "gen": "pnpm run gen:css",  
  "gen:css": "tcm components",
  "lint": "pnpm run lint:biome",
  "lint:biome": "biome check .",
  "postinstall": "shx cp ../../packages/db-structure/src/parser/schemarb/prism.wasm prism.wasm",
  "start": "next start"
}

}
